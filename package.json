{
  "name": "covid19-graphql",
  "version": "1.0.0",
  "description": "API de todos los casos de Covid-19 que hay en el todo el mundo",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon \"src/server.ts\" --exec \"ts-node\" src/server.ts -e ts,graphql,json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elbarbero/API_Worldwide_Cases_Covid19_GraphQL.git"
  },
  "keywords": [
    "covid",
    "covid19",
    "coronavirus",
    "world",
    "graphql",
    "API"
  ],
  "author": "Mario Barbero Santillan <mario.barbero89@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elbarbero/API_Worldwide_Cases_Covid19_GraphQL/issues"
  },
  "homepage": "https://github.com/elbarbero/API_Worldwide_Cases_Covid19_GraphQL#readme",
  "dependencies": {
    "apollo-datasource-rest": "^0.9.0",
    "apollo-server-express": "^2.13.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.14",
    "graphql-tools": "^6.0.1",
    "http": "0.0.1-security",
    "lodash": "^4.17.19",
    "ncp": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.152",
    "@types/node": "^14.0.5"
  }
}
